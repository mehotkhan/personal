{
  // https://v3.nuxtjs.org/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "sourceMap": false,
    "strict": true,
    "module": "es2022",
    "target": "es2017",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "types": ["@vueuse/nuxt", "unplugin-icons/types/vue"],
    "exclude": ["functions/**/*"]
  }
}
